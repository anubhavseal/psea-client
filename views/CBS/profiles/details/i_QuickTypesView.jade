.col-md-8
    .row
        ul.list-group
            li.col-md-4.list-group-item.quick-types-list(ng-repeat="quickPickType in quickPickTypes")
                input.cbs-input-box(type="checkbox" id="{{quickPickType.hierarchyName}}" ng-checked="{{quickPickType.selected}}" ng-model="quickPickType.selected")
                label(for="{{quickPickType.hierarchyName}}" style="font-family:'Lato', sans-serif !important;") &nbsp; {{quickPickType.hierarchyName}}
    button.btn.btn-success.btn-lg.pull-right#quick-pick-type(ng-click="updateQuickPickCount()") Apply