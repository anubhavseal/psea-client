doctype html
html
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible' content='IE=edge')
		meta(name='viewport' content='width=device-width')
		meta(name='csrf-token' content=_csrf)
		meta(name='description' content='')
		meta(name='author' content='')
		title PSEA
		//link(href='//fonts.googleapis.com/css?family=Noto+Sans:400,700', rel='stylesheet', type='text/css')
		link(href="https://fonts.googleapis.com/css?family=Lato" rel="stylesheet")
		!= css('lib/nvd3/nv.d3.css')
		!= css('lib/toastr/toastr.min.css')
		!= css('lib/angular-treeview/css/angular.treeview.css')
		!= css('nextgen/stylesheets/application')
		!= css('nextgen/stylesheets/custom')
		!= css('lib/simplepagination/simplePagination')
		!= css('font-awesome/font-awesome')
		!= css('nextgen/stylesheets/cbsCustom')
		!= js('application')
		!= js('lib/nextgen/Chart.js')
		!= js('lib/nextgen/app.js')
