	body(ng-app="global" ng-cloak="ng-cloak")
		.page-container
			.navigation-dock
				.left-pane
					include partials/menu
					// include partials/footer
			.nav
			main.app-main(style="background-color:white;")
				include partials/header
				.container-fluid
					block content
		.loading-spiner-holder.theme-loading-spiner-holder
			i.fa.fa-spinner.fa-pulse
			.message.white
				span#loaderMessage
		.overlay(data-loading)
			.three-quarters